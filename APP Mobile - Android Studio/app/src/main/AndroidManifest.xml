            android:parentActivityName=".Actvities.telas.tela_login"/>
        <activity
            android:name=".Actvities.telas.ListarClienteActivity"
            android:exported="true"
            android:parentActivityName=".Actvities.telas.MainActivity"/>
        <activity
            android:name=".Actvities.telas.tela_login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
