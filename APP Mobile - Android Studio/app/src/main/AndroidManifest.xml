            android:parentActivityName=".Actvities.telas.MainActivity"/>
        <activity
            android:name=".Actvities.telas.usuario_logado"
            android:exported="true"
            android:parentActivityName=".Actvities.telas.tela_login"/>
        <activity
            android:name=".Actvities.telas.MainActivity"
